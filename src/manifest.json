{
  "manifest_version": 2,
  "name": "Convert JIRA issue to feature name",
  "version": "0.3",

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],

  "browser_action": {
    "default_title": "Test Extension",
    "default_popup": "popup.html"
  },

  "permissions": ["activeTab"]
}